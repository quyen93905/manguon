<!DOCTYPE html>
<html>
  <head>
    <title>user3</title>
    <link rel='stylesheet' href='/public/stylesheets/style.css' />
  </head>
  <body>
    <h1>Danh Sách User3</h1>
    <a href="/form-add"><button type="submit">Thêm Mới</button></a>
    <table border="1">
      <tr>
        <th>IdDistrict</th>
        <th>IdVillage</th>
        <th>Address</th>
        <th>IsGetNotification </th>
        <th>IsVerified</th>
        <th>IsGetOpen</th>
        <th>Acction</th>
      </tr>
      <% userfours.forEach(item => {%>
        <tr>
          <td><%= item._id %></td>
          <td><%= item.IdVillage %></td>
          <td><%= item.Address %></td>
          <td><%= item.IsGetNotification %></td>
          <td><%= item.IsVerified %></td>
          <td><%= item.IsGetOpen %></td>
          <td><a href="/form-update/<%= item._id %>">Update</a>||<a href="/form-delete/<%= item._id %>">Delete</a></td>
        </tr>
      <%})%>
    </table>
  </body>
</html>
